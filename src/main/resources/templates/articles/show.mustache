{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
        {{#article}}  <!-- 모델에 등록된 데이터는 #으로 가져온다 -->
    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>
        {{/article}}
    </tbody>
    <a href="/articles">Go to Article List</a>
</table>

{{>layouts/footer}}